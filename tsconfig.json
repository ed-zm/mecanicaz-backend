{
  "compilerOptions": {
    "types": ["node"],
    "module": "commonjs",
    "target": "es5",
    "lib": ["es2017", "es6", "dom","esnext"],
    "sourceMap": true,
    "allowJs": false,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    // "baseUrl": ".",
    // "paths": {
    //   "*": [
    //     "*",
    //     "app/*"
    //   ]
    // },
  },
  "exclude": [
    "node_modules",
  ]
}

