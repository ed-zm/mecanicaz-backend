{
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "app/**/**.js" : {
            "when": "$(basename).tsx"
        },
        "app/**/*.js" : {
            "when": "$(basename).ts"
        },
        "app/**/*.js.map": {
            "when": "$(basename)"
        }
    }
}
